<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <!-- <tab id="rvprofiletab" translate="label" sortOrder="300">
            <label>Camping World</label>
        </tab> -->
        <section id="rvprofile" translate="label" sortOrder="340" showInDefault="1" showInWebsite="1" showInStore="1">
            <class>separator-top</class>
            <label>RV Profile</label>
            <tab>campingworld</tab>
            <resource>Camping_Auth::config_rvprofile</resource>
            <group id="general" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                <label>General Settings</label>
                <field id="rvprofile_path" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>RV Profile Path</label>
                    <comment>Any of the fields allow fully qualified URLs that end with '/' (slash) e.g. http://example.com/magento/</comment>
                </field>
            </group>
        </section>
    </system>
</config>
